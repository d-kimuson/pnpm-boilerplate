{
  "name": "@pnpm-boilerplate/helpers",
  "description": "shared helpers package",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js",
      "@pnpm-boilerplate/src": "./src/index.ts"
    }
  },
  "scripts": {
    "dev": "pnpm typecheck --watch",
    "build": "tsup --clean",
    "lint": "pnpm run:eslint .",
    "fix": "pnpm lint --fix",
    "typecheck": "tsc -p ./tsconfig.src.json --noEmit",
    "test": "vitest --run --shard=${TEST_SHARED:-1/1}",
    "test:watch": "vitest",
    "run:eslint": "eslint --cache --no-error-on-unmatched-pattern --flag unstable_ts_config"
  },
  "dependencies": {
    "neverthrow": "8.1.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/node": "22.9.0",
    "@typescript-eslint/eslint-plugin": "8.14.0",
    "@typescript-eslint/utils": "^8.14.0",
    "eslint": "9.15.0",
    "@pnpm-boilerplate/eslint-config": "workspace:*",
    "@pnpm-boilerplate/tsconfig": "workspace:*",
    "tsd": "0.31.2",
    "tsup": "8.0.2",
    "typescript": "5.6.3",
    "vitest": "2.1.9"
  }
}
