{
  "name": "backend",
  "description": "",
  "private": true,
  "version": "0.0.0",
  "main": "./dist/index.mjs",
  "scripts": {
    "ts:run": "pnpm tsx --tsconfig ./tsconfig.src.json",
    "dev": "pnpm /^dev:.*/",
    "dev:tsc": "pnpm build:tsc --watch",
    "dev:esbuild": "WATCH=true node ./scripts/build.mjs",
    "build": "pnpm /^build:.*/",
    "build:esbuild": "node ./scripts/build.mjs",
    "build:tsc": "tsc -p ./tsconfig.src.json",
    "lint": "eslint . --cache --no-error-on-unmatched-pattern",
    "fix": "pnpm lint --fix",
    "typecheck": "tsc -p ./tsconfig.src.json"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "better-typescript-lib": "^2.4.0",
    "esbuild": "^0.19.2",
    "eslint-config-custom": "workspace:*",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "tsconfig": "workspace:*",
    "tsx": "^3.12.8",
    "typescript": "^5.2.2"
  }
}
